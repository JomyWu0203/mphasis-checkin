<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- 配置文件基本資訊 -->
    <key>PayloadContent</key>
    <array>
        <!-- Web Clip 配置 -->
        <dict>
            <key>PayloadType</key>
            <string>com.apple.webClip.managed</string>
            <key>PayloadVersion</key>
            <integer>1</integer>
            <key>PayloadIdentifier</key>
            <string>com.mphasis.checkin.webclip</string>
            <key>PayloadUUID</key>
            <string>A1B2C3D4-E5F6-7890-ABCD-EF1234567890</string>
            <key>PayloadDisplayName</key>
            <string>Mphasis 打卡系統</string>
            <key>PayloadDescription</key>
            <string>Mphasis CheckIN 企業打卡系統 Web Clip</string>
            
            <!-- Web Clip 設定 -->
            <key>Label</key>
            <string>Mphasis打卡</string>
            <key>URL</key>
            <string>https://enterprise-apps.mphasis.com/ess/v1/#/home</string>
            <key>Icon</key>
            <data>
                <!-- Base64 編碼的 PNG 圖示 (建議 180x180 pixels) -->
                
            </data>
            <key>IsRemovable</key>
            <true/>
            <key>FullScreen</key>
            <true/>
            <key>Precomposed</key>
            <true/>
        </dict>
        
        <!-- VPN 配置（可選） -->
        <dict>
            <key>PayloadType</key>
            <string>com.apple.vpn.managed</string>
            <key>PayloadVersion</key>
            <integer>1</integer>
            <key>PayloadIdentifier</key>
            <string>com.mphasis.checkin.vpn</string>
            <key>PayloadUUID</key>
            <string>B2C3D4E5-F6G7-8901-BCDE-F23456789012</string>
            <key>PayloadDisplayName</key>
            <string>Mphasis VPN Configuration</string>
            <key>PayloadDescription</key>
            <string>VPN configuration for Mphasis CheckIN system</string>
            
            <!-- VPN 設定 -->
            <key>UserDefinedName</key>
            <string>Mphasis CheckIN VPN</string>
            <key>VPNType</key>
            <string>IKEv2</string>
            <key>IKEv2</key>
            <dict>
                <key>RemoteAddress</key>
                <string>vpn.mphasis.com</string>
                <key>RemoteIdentifier</key>
                <string>vpn.mphasis.com</string>
                <key>LocalIdentifier</key>
                <string>mphasis-user</string>
                <key>AuthenticationMethod</key>
                <string>SharedSecret</string>
                <key>SharedSecret</key>
                <string>your-shared-secret</string>
                <key>PayloadCertificateUUID</key>
                <string>certificate-uuid-here</string>
            </dict>
            <key>OnDemandEnabled</key>
            <integer>1</integer>
            <key>OnDemandRules</key>
            <array>
                <dict>
                    <key>Action</key>
                    <string>Connect</string>
                    <key>URLStringProbe</key>
                    <string>https://mphasisone.mphasis.com</string>
                </dict>
            </array>
        </dict>
        
        <!-- 安全性設定 -->
        <dict>
            <key>PayloadType</key>
            <string>com.apple.applicationaccess</string>
            <key>PayloadVersion</key>
            <integer>1</integer>
            <key>PayloadIdentifier</key>
            <string>com.mphasis.checkin.restrictions</string>
            <key>PayloadUUID</key>
            <string>C3D4E5F6-G7H8-9012-CDEF-345678901234</string>
            <key>PayloadDisplayName</key>
            <string>Mphasis Security Settings</string>
            <key>PayloadDescription</key>
            <string>Security restrictions for Mphasis CheckIN</string>
            
            <!-- 允許企業應用安裝 -->
            <key>allowUIAppInstallation</key>
            <true/>
            <key>allowUIConfigurationProfileInstallation</key>
            <true/>
            <key>allowUntrustedTLSPrompt</key>
            <false/>
        </dict>
    </array>
    
    <!-- 配置文件主要資訊 -->
    <key>PayloadDisplayName</key>
    <string>Mphasis CheckIN 企業配置</string>
    <key>PayloadDescription</key>
    <string>Mphasis CheckIN 企業打卡系統配置文件，包含 Web Clip、VPN 和安全性設定</string>
    <key>PayloadIdentifier</key>
    <string>com.mphasis.checkin.enterprise.config</string>
    <key>PayloadOrganization</key>
    <string>Mphasis Limited</string>
    <key>PayloadRemovalDisallowed</key>
    <false/>
    <key>PayloadType</key>
    <string>Configuration</string>
    <key>PayloadUUID</key>
    <string>D4E5F6G7-H8I9-0123-DEFG-456789012345</string>
    <key>PayloadVersion</key>
    <integer>1</integer>
    
    <!-- 加密和簽名 -->
    <key>PayloadScope</key>
    <string>User</string>
    <key>RemovalDate</key>
    <date>2030-12-31T23:59:59Z</date>
    
    <!-- 自動移除設定 -->
    <key>DurationUntilRemoval</key>
    <real>86400</real>
    
    <!-- 企業簽名資訊 -->
    <key>HasRemovalPasscode</key>
    <false/>
    <key>IsEncrypted</key>
    <false/>
</dict>
</plist>
